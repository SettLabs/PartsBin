<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Capacitors>
	<database>
		<query name="purchases">
			INSERT INTO global (mpn, stock)
			SELECT 
			mpn,
			SUM(quantity) as total_quantity
			FROM purchases
			GROUP BY mpn
			ON CONFLICT(mpn) 
			DO UPDATE SET stock = excluded.stock;
		</query>
		<table>
		CREATE TABLE capacitors (
			id INTEGER PRIMARY KEY AUTOINCREMENT,
			name TEXT NOT NULL,
			value TEXT,
			description TEXT,
			
			-- Manufacturer Info
			mpn TEXT NOT NULL,
			manufacturer TEXT NOT NULL,
			
			-- Supplier SKUs
			lcsc TEXT,		
			mouser TEXT,
			farnell TEXT,
			
			-- Electrical Specifications
			capacitance TEXT,
			voltage_rating TEXT,
			tolerance TEXT,
			dielectric TEXT,
			esr TEXT,
			ripple_current TEXT,
			
			-- Measured Characteristics
			cap_at_3v3 TEXT,
			cap_at_5v TEXT, 
			cap_at_12v TEXT,
			
			-- Other
			misc TEXT,
			
			-- Metadata
			enriched BOOLEAN DEFAULT FALSE,
			last_updated TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
			
			-- Indexes for faster lookups
			UNIQUE(mpn)
		);
		</table>
		<columns>name,value,description,lcsc,mpn,manufacturer,esr,misc,tolerance,cap_at_12v,voltage_rating,dielectric,cap_at_3v3,cap_at_5v,ripple_current,mouser,cap_at_3v3,capacitance,farnell,enriched</columns>
	</database>
